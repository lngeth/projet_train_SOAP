<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TrainBookingService WSDL service -->

  <service name="TrainBooking" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://main"/>
      <parameter name="wsdlServiceElement" value="TrainBookingService"/>
      <parameter name="schemaQualified" value="http://main"/>
      <parameter name="wsdlServicePort" value="TrainBooking"/>
      <parameter name="className" value="main.TrainBookingSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TrainBooking"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getTrain" qname="operNS:getTrain" xmlns:operNS="http://main" returnQName="retNS:getTrainReturn" xmlns:retNS="http://main" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getTrain"/>

  </service>
</deployment>
